{
  "name": "kweb-test",
  "version": "1.0.0",
  "description": "for fun",
  "main": "index.js",
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/index.js --delay 2",
    "dev:assets": "set WEBPACK_ENV=development&&cd src&&webpack -w",
    "build:assets": "set WEBPACK_ENV=production&&cd src&&webpack",
    "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.config.js'",
    "copyAll": "cp -R src/static build&&cp -R src/views build",
    "build": "npm run build:server&&npm run build:assets&&npm run copyAll",
    "start": "PRODUCTION=true node build/index.js",
    "prestart": "npm run build"
  },
  "author": "KMK",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/cli": "^7.8.4",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.1.0",
    "body-parser": "^1.19.0",
    "brew": "0.0.8",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "css-loader": "^3.5.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "helmet": "^3.22.0",
    "moment": "^2.25.1",
    "moment-timezone": "^0.5.28",
    "mongo": "^0.1.0",
    "mongodb": "^3.5.7",
    "mongoose": "^5.9.11",
    "morgan": "^1.10.0",
    "node-sass": "^4.14.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "postcss": "^7.0.28",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.4",
    "rm": "^0.1.8",
    "sass-loader": "^8.0.2",
    "webpack": "^4.43.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.3",
    "webpack-cli": "^3.3.11"
  }
}
